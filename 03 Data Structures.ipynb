    "    print(list1)\n",
    "    list1.sort(key=numberOfVowels, reverse=True)\n",
    "    print(list1)\n",
    "def numberOfVowels(word):\n",
    "    vowels = ('a', 'e', 'i', 'o', 'u')\n",
    "    total = 0\n",
    "    for vowel in vowels:\n",
    "        total += word.count(vowel)\n",
    "    return total\n",
    "main()"
